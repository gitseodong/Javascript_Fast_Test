let arr = [2, 4, 3, 1, 9, 6, 8, 7, 5];

function insertionSort(arr) {
  for (let i = 1; i < arr.length; i++) {
    for (let j = i; j >= 1; j--) {
      if (arr[j] < arr[j-1]) {
        const temp = arr[j];
        arr[j] = arr[j-1];
        arr[j-1] = temp;
      }
    }
  }
}
insertionSort(arr)
console.log(arr);
