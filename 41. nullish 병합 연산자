// Nullish 병합(Nullish coalescing)
// or 연산자 : 거짓이면 오른쪽으로 넘어가면서 체크.
// nullish 병합 연산자 : null 또는 undefined이면 오른쪽으로 넘어가면서 체크.

let n = 0;

const num1 = n || 5;
console.log(num1); // 5

const num2 = n ?? 5;
console.log(num2); // 0

n = null;
const num3 = n ?? 5;
console.log(num3); // 5

n = undefined;
const num4 = n ?? 5;
console.log(num4); // 5
